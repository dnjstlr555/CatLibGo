{
    "manifest_version": 2,
    "name": "Proxy Redirect",
    "version": "1.0",
    "description": "Redirects the current tab to a proxy server",
    "icons": {
        "48": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}